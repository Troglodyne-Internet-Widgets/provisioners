mkdir -p [% to %]/[% domain %]
# We install the key to whatever user ran bin/provision from trog-provisioner.
# This needs to be set up in your global config.
rsync -av [% transfer_user %]@[% hv_ip %]:[% from %]/[% domain %]/ [% to %]/[% domain %]/
chown -R [% user %]:[% admin_user %] [% to %]/[% domain %]
chmod -R 0750 [% to %]/[% domain %]
